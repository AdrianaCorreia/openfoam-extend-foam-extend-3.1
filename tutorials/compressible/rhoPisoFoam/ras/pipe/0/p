FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 80e5;

boundaryField
{
     INLET
    {
   type            zeroGradient;
    }
    OUTLET
    {
        type            fixedValue;
        value           uniform 8e+06;
    }

    WALL_1
    {
        type            zeroGradient;
    }
    WALL_2
    {
        type            zeroGradient;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


